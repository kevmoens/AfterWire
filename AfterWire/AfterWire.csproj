<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>After Wire</Title>
    <Authors>Kevin Moens</Authors>
    <Copyright>2025</Copyright>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/kevmoens/AfterWire</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ServiceProvider;Depedency Injection;Microsoft.Extensions.DependencyInjection</PackageTags>
    <PackageReleaseNotes>Initial Revision</PackageReleaseNotes>
    <EnforceCodeStyleInBuild>False</EnforceCodeStyleInBuild>
    <PackageProjectUrl>https://github.com/kevmoens/AfterWire</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.5" />
  </ItemGroup>

</Project>
